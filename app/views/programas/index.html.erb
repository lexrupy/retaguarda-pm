<p id="notice"><%= notice %></p>

<h1>Listing Programas</h1>

<table>
  <thead>
    <tr>
      <th>Descricao</th>
      <th>Data inicio</th>
      <th>Data fim</th>
      <th>Unidade</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @programas.each do |programa| %>
      <tr>
        <td><%= programa.descricao %></td>
        <td><%= programa.data_inicio %></td>
        <td><%= programa.data_fim %></td>
        <td><%= programa.unidade %></td>
        <td><%= link_to 'Show', programa %></td>
        <td><%= link_to 'Edit', edit_programa_path(programa) %></td>
        <td><%= link_to 'Destroy', programa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Programa', new_programa_path %>
